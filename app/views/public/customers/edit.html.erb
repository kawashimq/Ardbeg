<%= form_for @customer, url: public_customer_path do |f| %>
  <div class="text-center">
    <label>Name</label>
    <div><%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"名前", class:"w-25" %></div>
    <label class="mt-2">ProfileImage</label>
    <div><%= f.file_field :profile_image, placeholder: "プロフィール画像", accept: "image/*", class:"w-25" %></div>  
    <div><%= f.submit "保存",class:"mt-4 btn w-25 btn-success" %></div>
  </div>
<% end %>