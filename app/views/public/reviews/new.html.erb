<h1>画像投稿フォーム</h1>
  <%= form_with model: @review, url:public_reviews_path do |f| %>
    <h4>画像投稿</h4>
      <%= f.file_field :image, accept: "image/*" %>
    <h4>商品名</h4>
      <%= f.text_field :item %>
    <h4>口コミ</h4>
      <%= f.text_area :content %>
    <h4>原産地</h4>
    <%= f.select :production_area_id, options_from_collection_for_select(ProductionArea.all, :id, :genre_name), {} %>
    
    <%= f.submit '投稿' %>
  <% end %>